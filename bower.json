{
  "name": "grunt-bower-installer",
  "version": "0.1.0",
  "dependencies": {
    "jquery": "~1.9.1",
    "jquery-ui": "~1.10.2"
  },
  "install": {
    "path": "tmp",
    "sources": {
      "jquery": "*.js",
      "jquery-ui": [
        "ui/jquery-ui.custom.js",
        "themes/smoothness/jquery-ui.css"
      ]
    }
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "components"
  ]
}